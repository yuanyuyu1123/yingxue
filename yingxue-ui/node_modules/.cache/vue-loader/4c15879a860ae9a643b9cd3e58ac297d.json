    <div class=\"orders\">\n\n                <table class=\"order-item\" v-for=\"(order,index) in myOrder.records\" :key=\"order.id\">\n                    <thead>\n                        <tr>\n                            <th colspan=\"5\">\n                                <span class=\"ordertitle\">{{order.createTime}}　订单编号：{{order.outTradeNo}} <span class=\"pull-right delete\"><img src=\"../images/delete.png\"></span></span>\n                            </th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        <tr v-for=\"(detail,index) in order.orderDetailList\" :key=\"detail.id\">\n                            <td width=\"60%\">\n                                <div class=\"typographic\">\n                                    <img :src=\"detail.imgUrl\" style=\"width:100px\">\n                                    <a href=\"#\" class=\"block-text\">{{detail.skuName}}</a>\n                                    <span>x{{detail.skuNum}}</span>\n                                    <a href=\"#\" class=\"service\">售后申请</a>\n                                </div>\n                            </td>\n                            <td :rowspan=\"order.orderDetailList.length\" width=\"8%\" class=\"center\" v-if=\"index==0\">{{order.consignee}}</td>\n                            <td :rowspan=\"order.orderDetailList.length\" width=\"13%\" class=\"center\" v-if=\"index==0\">\n                                <ul class=\"unstyled\">\n                                    <li>总金额¥{{detail.orderPrice}}</li>\n                                    <li>在线支付</li>\n\n                                </ul>\n                            </td>\n                            <td :rowspan=\"order.orderDetailList.length\" width=\"8%\" class=\"center\" v-if=\"index==0\">\n                                <a href=\"#\" class=\"btn\">{{order.orderStatusName}} </a>\n                            </td>\n                            <td :rowspan=\"order.orderDetailList.length\" width=\"13%\" class=\"center\" v-if=\"index==0\">\n                                <ul class=\"unstyled\">\n                                    <li>\n                                        <a href=\"mycomment.html\" target=\"_blank\">评价|晒单</a>\n                                    </li>\n\n                                </ul>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n            <div class=\"choose-order\">\n                <!-- 分页 -->\n                <Pagination :pageNo=\"page\" :pageSize=\"limit\" :total=\"myOrder.total\" :continues=\"5\" @getPageNo=\"getPageNo\" />\n\n            </div>\n        </div>\n        <!--猜你喜欢-->\n        <div class=\"like\">\n            <h4 class=\"kt\">猜你喜欢</h4>\n            <ul class=\"like-list\">\n                <li class=\"likeItem\">\n                    <div class=\"p-img\">\n                        <img src=\"../images/itemlike01.png\" />\n                    </div>\n                    <div class=\"attr\">\n                        <em>DELL戴尔Ins 15MR-7528SS 15英寸 银色 笔记本</em>\n                    </div>\n                    <div class=\"price\">\n                        <em>¥</em>\n                        <i>3699.00</i>\n                    </div>\n                    <div class=\"commit\">已有6人评价\n                    </div>\n                </li>\n                <li class=\"likeItem\">\n                    <div class=\"p-img\">\n                        <img src=\"../images/itemlike02.png\" />\n                    </div>\n                    <div class=\"attr\">\n                        Apple苹果iPhone 6s/6s Plus 16G 64G 128G\n                    </div>\n                    <div class=\"price\">\n                        <em>¥</em>\n                        <i>4388.00</i>\n                    </div>\n                    <div class=\"commit\">已有700人评价\n                    </div>\n                </li>\n                <li class=\"likeItem\">\n                    <div class=\"p-img\">\n                        <img src=\"../images/itemlike03.png\" />\n                    </div>\n                    <div class=\"attr\">DELL戴尔Ins 15MR-7528SS 15英寸 银色 笔记本\n                    </div>\n                    <div class=\"price\">\n                        <em>¥</em>\n                        <i>4088.00</i>\n                    </div>\n                    <div class=\"commit\">已有700人评价\n                    </div>\n                </li>\n                <li class=\"likeItem\">\n                    <div class=\"p-img\">\n                        <img src=\"../images/itemlike04.png\" />\n                    </div>\n                    <div class=\"attr\">DELL戴尔Ins 15MR-7528SS 15英寸 银色 笔记本\n                    </div>\n                    <div class=\"price\">\n                        <em>¥</em>\n                        <i>4088.00</i>\n                    </div>\n                    <div class=\"commit\">已有700人评价\n                    </div>\n                </li>\n            </ul>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'MyOrder',\n    data() {\n        return {\n            page: 1,//当前第几页\n            limit: 2,//一页多少个\n            myOrder: {}\n        }\n    },\n    mounted() {\n        this.getData();\n    },\n    methods: {\n        //获取订单数据方法\n        async getData() {\n            const { page, limit } = this;\n            let result = await this.$API.redMyOrderList(page, limit);\n            if (result.code == 200) {\n                this.myOrder = result.data;\n            }\n        },\n        //获取当前点击页\n        getPageNo(page){\n            this.page=page;\n              this.getData();\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"less\">\n</style>\n"]}]}