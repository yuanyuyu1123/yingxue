    <div class=\"register\">\n      <h3>\n        注册新用户\n        <span class=\"go\"\n          >我有账号，去 <a href=\"login.html\" target=\"_blank\">登陆</a>\n        </span>\n      </h3>\n      <div class=\"content\">\n        <label>手机号:</label>\n        <!--  name=\"phone\":验证的是哪一个， v-validate校验-->\n        <input\n          placeholder=\"请输入你的手机号\"\n          v-model=\"phone\"\n          name=\"phone\"\n          v-validate=\"{ required: true, regex: /^1\\d{10}$/ }\"\n          :class=\"{ invalid: errors.has('phone') }\"\n        />\n        <span class=\"error-msg\">{{ errors.first(\"phone\") }}</span>\n      </div>\n      <div class=\"content\">\n        <label>验证码:</label>\n          <input\n          placeholder=\"请输入验证码\"\n          v-model=\"code\"\n          name=\"code\"\n          v-validate=\"{ required: true, regex: /\\d{6}/ }\"\n          :class=\"{ invalid: errors.has('code') }\"\n        />\n        <button style=\"width: 100px; height: 38px\" @click=\"getCode\">\n          获取验证码\n        </button>\n        <span class=\"error-msg\">{{ errors.first(\"code\") }}</span>\n      </div>\n      <div class=\"content\">\n        <label>登录密码:</label>\n         <input\n          placeholder=\"请输入你的登录密码\"\n          v-model=\"password\"\n          name=\"password\"\n          v-validate=\"{ required: true, regex: /^[0-9A-Za-z]{8,20}$/ }\"\n          :class=\"{ invalid: errors.has('password') }\"\n        />\n        <span class=\"error-msg\">{{ errors.first(\"password\") }}</span>\n      </div>\n      <div class=\"content\">\n        <label>确认密码:</label>\n         <input\n          placeholder=\"请输入确认密码\"\n          v-model=\"password1\"\n          name=\"password1\"\n          v-validate=\"{ required: true, is:password}\"\n          :class=\"{ invalid: errors.has('password1') }\"\n        />\n        \n        <span class=\"error-msg\">{{ errors.first(\"password1\") }}</span>\n      </div>\n      <div class=\"controls\">\n         <input\n           name=\"agree\" \n           type=\"checkbox\"\n           :checked=\"agree\"\n            v-validate=\"{ required: true, tongyi:true}\"\n            :class=\"{ invalid: errors.has('agree') }\"\n        />\n      \n        <span>同意协议并注册《尚品汇用户协议》</span>\n        <span class=\"error-msg\">{{ errors.first(\"agree\") }}</span>\n      </div>\n      <div class=\"btn\">\n        <button @click=\"userRegister\">完成注册</button>\n      </div>\n    </div>\n\n    <!-- 底部 -->\n    <div class=\"copyright\">\n      <ul>\n        <li>关于我们</li>\n        <li>联系我们</li>\n        <li>联系客服</li>\n        <li>商家入驻</li>\n        <li>营销中心</li>\n        <li>手机尚品汇</li>\n        <li>销售联盟</li>\n        <li>尚品汇社区</li>\n      </ul>\n      <div class=\"address\">地址：北京市昌平区宏福科技园综合楼6层</div>\n      <div class=\"beian\">京ICP备19006430号</div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      //收集表单数据\n      phone: \"\", //手机号\n      code: \"\", //验证码\n      password: \"\", //密码\n      password1: \"\", //确认密码\n      agree: true, //是否同意\n    };\n  },\n  methods: {\n    async getCode() {\n      try {\n        const { phone } = this;\n        phone && (await this.$store.dispatch(\"getCode\", phone));\n        this.code = this.$store.state.user.code;\n      } catch (error) {\n        alert(error.message);\n      }\n    },\n    async userRegister() {\n      //保证验证通过了才能发请求 \n       const success = await this.$validator.validateAll();\n       console.log(success)\n       if(!success) return;\n      //全部表单验证成功，在向服务器发请求，进行祖册\n      //只要有一个表单没有成功，不会发请求\n       console.log(11)\n      try {\n        //注册成功\n        const { phone, code, password, password1 } = this;\n        await this.$store.dispatch(\"userRregister\", {\n            phone,\n            code,\n            password,\n            password1,\n          });\n        this.$router.push(\"/login\");\n      } catch (error) {}\n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.register-container {\n  .register {\n    width: 1200px;\n    height: 445px;\n    border: 1px solid rgb(223, 223, 223);\n    margin: 0 auto;\n\n    h3 {\n      background: #ececec;\n      margin: 0;\n      padding: 6px 15px;\n      color: #333;\n      border-bottom: 1px solid #dfdfdf;\n      font-size: 20.04px;\n      line-height: 30.06px;\n\n      span {\n        font-size: 14px;\n        float: right;\n\n        a {\n          color: #e1251b;\n        }\n      }\n    }\n\n    div:nth-of-type(1) {\n      margin-top: 40px;\n    }\n\n    .content {\n      padding-left: 390px;\n      margin-bottom: 18px;\n      position: relative;\n\n      label {\n        font-size: 14px;\n        width: 96px;\n        text-align: right;\n        display: inline-block;\n      }\n\n      input {\n        width: 270px;\n        height: 38px;\n        padding-left: 8px;\n        box-sizing: border-box;\n        margin-left: 5px;\n        outline: none;\n        border: 1px solid #999;\n      }\n\n      img {\n        vertical-align: sub;\n      }\n\n      .error-msg {\n        position: absolute;\n        top: 100%;\n        left: 495px;\n        color: red;\n      }\n    }\n\n    .controls {\n      text-align: center;\n      position: relative;\n\n      input {\n        vertical-align: middle;\n      }\n\n      .error-msg {\n        position: absolute;\n        top: 100%;\n        left: 495px;\n        color: red;\n      }\n    }\n\n    .btn {\n      text-align: center;\n      line-height: 36px;\n      margin: 17px 0 0 55px;\n\n      button {\n        outline: none;\n        width: 270px;\n        height: 36px;\n        background: #e1251b;\n        color: #fff !important;\n        display: inline-block;\n        font-size: 16px;\n      }\n    }\n  }\n\n  .copyright {\n    width: 1200px;\n    margin: 0 auto;\n    text-align: center;\n    line-height: 24px;\n\n    ul {\n      li {\n        display: inline-block;\n        border-right: 1px solid #e4e4e4;\n        padding: 0 20px;\n        margin: 15px 0;\n      }\n    }\n  }\n}\n</style>\n"]}]}