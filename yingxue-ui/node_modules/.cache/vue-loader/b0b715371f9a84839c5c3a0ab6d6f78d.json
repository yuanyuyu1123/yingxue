xt-align: center;\n                                line-height: 26px;\n                            }\n                        }\n\n                        img {\n                            width: 100%;\n                        }\n                    }\n\n                    .floorBanner {\n                        width: 330px;\n                        height: 100%;\n                    }\n\n                    .split {\n                        width: 220px;\n                        height: 100%;\n                        position: relative;\n\n                        .floor-x-line {\n                            position: absolute;\n                            background: #e4e4e4;\n                            width: 220px;\n                            height: 1px;\n                            top: 180px;\n                        }\n\n                        .floor-conver-pit {\n                            width: 100%;\n                            height: 50%;\n\n                            img {\n                                width: 100%;\n                                height: 100%;\n                                transition: all 400ms;\n\n                                &:hover {\n                                    opacity: 0.8;\n                                }\n                            }\n                        }\n                    }\n\n                    .center {\n                        border: 1px solid #e4e4e4;\n                    }\n                }\n            }\n        }\n    }\n}\n</style>\n"]}]}